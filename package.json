{
  "name": "model-blueprint",
  "version": "0.1.3",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./withCursor": {
      "import": "./dist/withCursor.js",
      "types": "./dist/withCursor.d.ts"
    },
    "./createId": {
      "import": "./dist/createId.js",
      "types": "./dist/createId.d.ts"
    },
    "./dropQuery": {
      "import": "./dist/dropQuery.js",
      "types": "./dist/dropQuery.d.ts"
    }
  },
  "scripts": {
    "prepare": "husky",
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:check": "eslint . --max-warnings=0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepublishOnly": "npm run format && npm run lint && npm run test && npm run build"
  },
  "pre-commit": [
    "lint"
  ],
  "author": "Roman Jankowski",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Em3ODMe/model-blueprint.git"
  },
  "homepage": "https://github.com/Em3ODMe/model-blueprint#readme",
  "bugs": {
    "url": "https://github.com/Em3ODMe/model-blueprint/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "description": "model-blueprint",
  "dependencies": {
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@paralleldrive/cuid2": "^3.3.0",
    "@eslint/js": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.0.0",
    "husky": "^9.1.7",
    "prettier": "^3.8.1",
    "tsup": "^8.5.1",
    "typescript": "^5.0.0",
    "vitest": "^4.0.18"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "database",
    "procedure",
    "model",
    "typescript",
    "nodejs"
  ]
}
